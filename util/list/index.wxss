.content {
    width: 100%;
    background: #fff;
    margin-top: 168rpx;
    overflow: hidden;
    min-height: 100vh;
}
.first{
  color: red
}
.list {
    display: flex;
    flex-direction: row;
    padding: 30rpx 0;
    margin: 0 15px;
    border-bottom: 1rpx solid #ddd;
    position: relative;
}

.list:last-child {
    border: 0;
}

.list_left {
    width: 240rpx;
    margin-right: 30rpx;
    height: 216rpx;
    position: relative;
    overflow: hidden;
}

.list_left image {
    width: 240rpx;
    height: 216rpx;
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    border-radius: 8rpx;
    overflow: hidden;
}

.list_left image.play_icon {
    width: 72rpx;
    height: 72rpx;
    left: auto;
    right: 0;
    bottom: 0;
    top: auto;
}

.list_right {
    flex: 1;
    width: 420rpx;
}

.list_right .title {
    margin-bottom: 24rpx;
    display: flex;
    flex-direction: row;
    height: 48rpx;
    overflow: hidden;
}

.list_right .title text {
    font-size: 34rpx;
    line-height: 48rpx;
    color: #2f2f2f;
    display: block;
    white-space: nowrap;
}

.list_right .title text.areaName {
    max-width: 240rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_right .mess {
    margin-bottom: 16rpx;
    height: 34rpx;
    line-height: 34rpx;
    display: flex;
}

.list_right .mess text {
    color: #888;
    font-size: 24rpx;
    line-height: 34rpx;
    display: inline-block;
}

.list_right .mess text.price {
    color: #ff8e20;
    text-align: right;
    flex: 1;
}

.list_right .mess .price text {
    font-size: 34rpx;
    color: #ff8e20;
    width: auto;
    display: inline-block;
    margin-right: 7rpx;
}

.list_right .address {
    display: flex;
    width: 420rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 34rpx;
    line-height: 34rpx;
    font-size: 24rpx;
    color: #888;
    margin-bottom: 30rpx;
}

.list_right .address text {
    width: 388rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list_right .address image {
    display: inline-block;
    width: 24rpx;
    height: 24rpx;
    margin-top: 4rpx;
    margin-right: 8rpx;
}

.list_right .label {
    height: 32rpx;
    line-height: 32rpx;
    display: flex;
    flex-direction: row;
}

.list_right .label text {
    display: block;
    color: #888;
    font-size: 20rpx;
    height: 32rpx;
    line-height: 32rpx;
    padding: 0 12rpx;
    background: #f2f6fd;
    margin-right: 16rpx;
    border-radius: 8rpx;
}

.status3 {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    z-index: 2;
    background: rgba(255,255,255,0.5);
}

.status3 image {
    display: block;
    width: 64rpx;
    height: 175rpx;
    margin-top: 52rpx;
    margin-left: 76rpx;
}

.noMore {
    padding: 10rpx 0;
    background: #fff;
}

.noMore text {
    display: block;
    text-align: center;
    color: #666;
    font-size: 26rpx;
}

.noHouse {
    min-height: 100vh;
    width: 100%;
    background: #fff;
    overflow: hidden;
}

.noHouse image {
    display: block;
    width: 400rpx;
    height: 400rpx;
    margin: 100rpx auto 0;
}

.noHouse text {
    display: block;
    text-align: center;
    color: #666;
    font-size: 26rpx;
}

.delete {
    position: absolute;
    overflow: hidden;
    right: -95px;
    top: 0;
    width: 80px;
    height: 278rpx;
    background: #f13642;
    display: flex;
    justify-content: center;
    align-items: center;
}

.delete text {
    display: block;
    text-align: center;
    color: #fff;
    font-size: 34rpx;
}

.no-mess-title {
    margin-top: 64rpx;
    color: #8998A0;
}

.distance {
    display: flex;
    align-items: center;
    margin-top: 24rpx;
}

.distance .section {
    text-align: center;
    margin-right: 45rpx;
}

.distance .section image {
    width: 44rpx;
    height: 44rpx;
}

.distance .section text {
    display: block;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #8998A0;
    letter-spacing: 0;
    line-height: 40rpx;
}